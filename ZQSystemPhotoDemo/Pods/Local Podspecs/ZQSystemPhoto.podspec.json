{
  "name": "ZQSystemPhoto",
  "version": "1.0.0",
  "summary": "访问系统相册的组件",
  "description": "通过该组件，可以访问系统相册，该组件仿照微信开发，支持照片的多选，单选。",
  "homepage": "https://github.com/caozhiqiang1002",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "caozhiqiang1002": "1053524532@qq.com"
  },
  "source": {
    "git": "https://github.com/caozhiqiang1002/ZQSystemPhoto.git",
    "tag": "1.0.0"
  },
  "public_header_files": "Core/*.{h}",
  "private_header_files": "Private/**/*.{h}",
  "prefix_header_file": "Private/ZQSystemPhoto_Header.pch",
  "resources": "Private/Resources/*.{png}",
  "requires_arc": true,
  "platforms": {
    "ios": "9.0"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Core/*.{h,m}"
    },
    {
      "name": "Private",
      "subspecs": [
        {
          "name": "Controller",
          "source_files": "Private/Controller/*.{h,m}"
        },
        {
          "name": "Manager",
          "source_files": "Private/Manager/*.{h,m}"
        },
        {
          "name": "Other",
          "source_files": "Private/Other/*.{h,m}"
        },
        {
          "name": "View",
          "source_files": "Private/View/*.{h,m}"
        }
      ]
    }
  ]
}
